<%= form_with model: @image do |form| %>

    <%= form.label :title %>
    <%= form.text_field :title %>
    <% @image.errors.full_messages_for(:title).each do |message| %>
      <div><%= message %></div>
    <% end %>

    <%= form.label :file %>
    <%= form.file_field :file %>
    <% @image.errors.full_messages_for(:file).each do |message| %>
      <div><%= message %></div>
    <% end %>

    <%= form.submit %>

    
<% end %>